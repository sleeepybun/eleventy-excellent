<link
  rel="preload"
  href="/assets/fonts/work-sans/WorkSans-Regular.ttf"
  as="font"
  type="font/ttf"
  crossorigin
/>

<link
  rel="preload"
  href="/assets/fonts/work-sans/WorkSans-Italic.ttf"
  as="font"
  type="font/ttf"
  crossorigin
/>

<!-- preloads in page data -->
{%- if preloads -%}
  <!-- prettier-ignore -->
  <link rel="preload"
			href="{{ preloads.href }}"
			as="{{ preloads.as }}"
			{% if preloads.type %}type={{ preloads.type }}{% endif %}{% if preloads.imagesrcset %}imagesrcset="{{ preloads.imagesrcset }}"{% endif %}
			{% if preloads.crossorigin %}crossorigin{% endif %} />
{% endif %}
